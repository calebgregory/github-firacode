{
  "manifest_version": 2,
  "name": "github-firacode",
  "version": "0.4.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://gist.github.com/*",
        "https://bitbucket.org/*",
        "http://tfs.actum.local/*"
      ],
      "css": ["github.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "FiraCode/*.eot",
    "FiraCode/*.eot",
    "FiraCode/*.woff2",
    "FiraCode/*.woff",
    "FiraCode/*.ttf"
  ],
  "permissions": ["activeTab"],
  "description": "A chrome extension that brings FiraCode font to github's code blocks.",
  "icons": {
    "128": "images/icon128.png"
  }
}
